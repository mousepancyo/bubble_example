package {	import flash.display.Sprite;	import flash.events.Event;	import flash.display.Bitmap;	import flash.display.BitmapData;	import flash.display.Shape;	public class Main extends Sprite	{		private var _bubble:Bubble = new Bubble(465, .3);				public function Main()		{			addChild(new Bitmap(new BitmapData(stage.stageWidth, stage.stageHeight, false, 0x003455)));			_bubble.x = stage.stageWidth * .5;  _bubble.y = stage.stageHeight - 10;			addChild(_bubble);			//			addEventListener(Event.ENTER_FRAME, update);		}				private function update(e:Event):void		{			_bubble.update();		}	}}